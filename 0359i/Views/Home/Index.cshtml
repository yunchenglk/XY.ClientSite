<script type="text/javascript">
    $(function () {
        //学员风采
        var obj_xyfc = $.GetApiData(apiurl + "/News/" + urlitems.xyfc + "?type=0&count=8");
        if (obj_xyfc.status == 1) {
            var slider_ht = "";
            $.each(obj_xyfc.data, function (i, n) {
                slider_ht += '<li>';
                slider_ht += '<a href="case_info_4_110.html">';
                slider_ht += '<div class="pic"><img src="' + n.SlidePicStr + '"></div>';
                slider_ht += '<div class="hover">';
                slider_ht += '<b></b>';
                slider_ht += '<div class="txt">';
                slider_ht += '<img src="/images/logo_small.png" alt="">';
