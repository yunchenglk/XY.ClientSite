                slider_ht += '</a>';
                slider_ht += '</li>';
            })
            $("#_xyfc_div").html(slider_ht);
        }
        //行业动态
        var obj_hydt = $.GetApiData(apiurl + "/News/" + urlitems.hydt + "?type=0&count=4");
        if (obj_hydt.status == 1) {
            var slider_ht = "";
            $.each(obj_hydt.data, function (i, n) {
                slider_ht += '<li>';
                slider_ht += '<div class="pad">';
                slider_ht += '<div class="txt">';
                slider_ht += '<span><em>' + n.CreateTime_.Day + '/' + n.CreateTime_.Month + '</em>' + n.CreateTime_.Year + '</span>';
                slider_ht += '<h3><a href="news_info_5_313.html">' + n.Title + '</a></h3>';
                slider_ht += '<p>' + (n.Short_Dec.length > 40 ? n.Short_Dec.substr(0, 40) + "..." : n.Short_Dec) + '</p>';
                slider_ht += '<a href="news_info_5_313.html" class="more"></a>';
                slider_ht += '</div>';
                slider_ht += '<div class="hover">';
                slider_ht += '<div class="img" style="background: url(' + n.SlidePicStr + ') 0 0 /100% 100% no-repeat;background: url(images/14568143499060.jpg) 0 0 no-repeat\9;"></div>';
                slider_ht += '<div class="pad">';
                slider_ht += '<h3><a href="news_info_5_313.html">' + n.Title + '</a></h3>';
                slider_ht += '<p>' + (n.Short_Dec.length > 40 ? n.Short_Dec.substr(0, 40) + "..." : n.Short_Dec) + '</p>';
                slider_ht += '</div>';
                slider_ht += '</div>';
                slider_ht += '</div>';
                slider_ht += '</li>';
            })
            $("#_hydt_div").html(slider_ht);
            $('.ul-news-i li').hover(function () {
                $(this).toggleClass('on');
            })
        }
    })
</script>
